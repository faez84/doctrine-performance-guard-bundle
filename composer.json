{
  "name": "faez84/doctrine-performance-guard-bundle",
  "type": "symfony-bundle",
  "description": "Doctrine Performance Guard: detect N+1 and slow/too-many queries in Symfony",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "symfony/http-kernel": "^6.4|^7.0",
    "symfony/http-foundation": "^6.4|^7.0",
    "symfony/event-dispatcher": "^6.4|^7.0",
    "symfony/console": "^6.4|^7.0",
    "doctrine/dbal": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "Faez84\\DoctrinePerformanceGuardBundle\\": "src/"
    }
  },
  "extra": {
    "symfony": {
      "allow-contrib": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
